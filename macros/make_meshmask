 writeu,numlec,'e1f  ',e1f
  writeu,numlec,'e2t  ',e2t
  writeu,numlec,'e2u  ',e2u
  writeu,numlec,'e2v  ',e2v
 writeu,numlec,'e2f  ',e2f‰
 writeu,numlec,'tmask',tmask
  umask=umask()
 vmask=vmask()
 fmask=fmask()
 writeu,numlec,'umask',umask
 writeu,numlec,'vmask',vmask
 writeu,numlec,'fmask',fmask
 writeu,numlec,'blank'
 writeu,numlec,'gdept',gdept
  writeu,numlec,'gdepw',gdepw
